{
  "compilerOptions": {
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "target": "es6",
    "sourceMap": false,
    "strict": true,
    "removeComments": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "../dist/"
  },
  "include": [
    "*.ts",
  ],
  "exclude": ["node_modules"]
}
